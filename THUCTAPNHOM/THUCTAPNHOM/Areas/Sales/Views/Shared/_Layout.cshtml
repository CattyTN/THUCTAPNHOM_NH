                                <div id="header-cart" class="block block-cart">
                                    <ul class="mini-products-list">
                                        @{
                                            if ((List<ItemInCart>)ViewBag.itemlist != null)
                                            {
                                                var all_amount = 0;
                                                var chietkhau = 0;
                                                foreach (var a in (List<ItemInCart>)ViewBag.itemlist)
                                                {
                                                    var amount = a.qty * a.price;
                                                    all_amount = all_amount + amount;

                                                    string amount_id = "amount" + a.product_id.ToString();
                                                    string qty_id = "qty" + a.product_id.ToString();
                                                    string price_id = "price" + a.product_id.ToString();
                                                    <li class="item">
                                                        <a class="product-image" href="/Sales/Product/Product_Detail?product_id=@a.product_id">
                                                            <img src="@a.image_link" alt="@a.name" title="" />
                                                        </a>
                                                        <div class="product-details">
                                                            <a href="" class="remove"><i id="@a.size" name="@a.product_id" class="anm anm-times-l remove" aria-hidden="true"></i></a>
                                                            <a href="/Sales/Product/Product_Detail?product_id=@a.product_id" class="edit-i remove"><i class="anm anm-edit" aria-hidden="true"></i></a>
                                                            <a class="pName" href="/Sales/Product/Product_Detail?product_id=@a.product_id">@a.name</a>
                                                            <div class="variant-cart">@a.size</div>
                                                            <div class="wrapQtyBtn">
                                                                <div class="qtyField">
                                                                    <span class="label">SL:</span>
                                                                    <a class="qtyBtn minus" href="javascript:void(0);"><i id="@a.product_id" class="fa anm anm-minus-r" aria-hidden="true"></i></a>
                                                                    <input type="text" id="@qty_id" name="quantity" value="@a.qty" class="product-form__input qty">
                                                                    <a class="qtyBtn plus" href="javascript:void(0);"><i id="@a.product_id" class="fa anm anm-plus-r" aria-hidden="true"></i></a>
                                                                </div>
                                                            </div>
                                                            <div class="priceRow">
                                                                <div class="product-price">
                                                                    <span id="@amount_id" class="money amount">@amount VND </span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="get_information d-none" id="@price_id">@a.price VND</div>
                                                        <div class="d-none" id="tongtien"> </div>
                                                        <div class="d-none" id="chietkhau"></div>
                                                    </li>
                                                }
                                                chietkhau = all_amount / 10;
                                                total = all_amount + chietkhau;
                                                <div class="d-none" id="all_amount"> @all_amount</div>
                                                <div class="d-none" id="hidden_div_id" name="@ViewBag.user_logined">@ViewBag.user_logined</div>
                                            }
                                            else
                                            {
                                                <h1>Chưa có sản phẩm nào!!</h1>
                                            }
                                        }



                                    </ul>
                                    <div class="total">
                                        <div class="total-in">
                                            <span class="label">Tổng tiền:</span><span class="product-price"><span id="total_final" class="money">@total VND</span></span>
                                        </div>
